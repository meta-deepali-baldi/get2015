<apex:page standardController="Student__c" renderAs="pdf">
  <apex:form >
   <apex:pageBlock title="Student Record">
      <apex:pageBlockSection columns="1">
         
      <apex:inputField label="Name" value="{! Student__c.Name}"/>
      <apex:inputField label="LastName" value="{! Student__c.LastName__c}"/>
       <apex:inputField label="DOB" value="{! Student__c.DOB__c}"/>
       <apex:inputField label="Class" value="{! Student__c.Class__c}"/>
        <apex:inputField label="Email" value="{! Student__c.email__c}"/>
       <apex:inputField label="Sex" value="{! Student__c.Sex__c}"/>
        <apex:inputField label="SpouseName" value="{! Student__c.Spouse_Name__c}"/>
        <apex:inputField label="Married" value="{! Student__c.Married__c}"/>
              
 </apex:pageBlockSection>
   </apex:pageBlock>  
  </apex:form> 
   
</apex:page>